  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-connect": "^5.7.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-rev": "^9.0.0",
    "gulp-rev-collector": "^1.3.1",
    "gulp-uglify": "^3.0.2",
    "prettier-eslint": "^9.0.1"
  },
  "browserslist": [
    "Android >= 4.4",
    "iOS >= 8",
    "ie >= 11",
    "chrome >= 37",
    "Firefox ESR",
    "not ie <= 8",
    "not dead"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base"
    ],
    "globals": {
      "$": true,
      "SVGA": true,
      "etpl": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3,
          "exclude": [
            "es.array.for-each",
            "es.array.index-of",
            "es.array.join",
            "es.array.slice",
            "es.number.constructor",
            "es.number.is-nan",
            "es.regexp.exec",
            "es.string.replace",
            "es.string.search",
            "es.string.split",
            "web.dom-collections.for-each",
            "es.array.map",
            "es.number.to-fixed"
          ]
        }
      ]
    ]
  }
